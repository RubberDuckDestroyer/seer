{
  "name": "seer",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "MODE_ENV=production node ./build/server.js",
    "server": "nodemon --ignore './client' --exec babel-node src/server.js --extensions \".ts\",\".js\"",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install && cd ../",
    "copy-shared-lib": "rm -rf ./client/src/libs && cp -R ./src/libs ./client/src/libs",
    "pretest": "npm run copy-shared-lib && eslint --ignore-path .eslintignore .",
    "test": "NODE_ENV=test mocha --require \"src/tests/babel-register.js\" --require @babel/register \"src\" --recursive --exit",
    "prebuild": "npm run copy-shared-lib && rm -rf ./build",
    "build": "babel -d ./build ./src",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RubberDuckDestroyer/SEER.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bibtex-parse": "^2.1.0",
    "bootstrap": "^4.4.1",
    "dotenv": "^8.2.0",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.25.3",
    "mongoose": "^5.8.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/plugin-transform-typescript": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "chai": "^4.2.0",
    "concurrently": "^5.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.1",
    "typescript": "^3.9.2"
  },
  "bugs": {
    "url": "https://github.com/RubberDuckDestroyer/SEER/issues"
  },
  "homepage": "https://github.com/RubberDuckDestroyer/SEER#readme"
}
